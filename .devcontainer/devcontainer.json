{
  "name": "Free Parking Dev Container",
  "image": "mcr.microsoft.com/vscode/devcontainers/javascript-node:0-20",
  "workspaceFolder": "/workspaces/free-parking",
  "postCreateCommand": "npm ci --no-audit --no-fund",
  "postStartCommand": "npm ci --no-audit --no-fund",
  "customizations": {
    "vscode": {
      "extensions": [
          "dbaeumer.vscode-eslint",
          "esbenp.prettier-vscode",
          "github.codespaces",
          "github.copilot",
          "github.copilot-chat",
          "github.github-vscode-theme",
          "github.vscode-pull-request-github",
          "luohe.js-debug-for-luohe-react",
          "ms-vscode.js-debug-nightly"
      ]
    }
  }
}

