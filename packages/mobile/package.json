{
  "name": "@free-parking/mobile",
  "version": "1.0.0",
  "description": "React Native Expo app for parking availability monitoring (iOS & Android)",
  "private": true,
  "main": "index.js",
  "scripts": {
    "dev": "npx expo start --offline",
    "dev:ios": "npx expo start --ios --offline",
    "dev:android": "npx expo start --android --offline",
    "build:ios": "eas build --platform ios",
    "build:android": "eas build --platform android",
    "build:preview": "eas build --platform android --profile preview",
    "submit:ios": "eas submit --platform ios",
    "submit:android": "eas submit --platform android",
    "eject": "npx expo prebuild",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint .",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@babel/runtime": "^7.28.4",
    "@expo/vector-icons": "^14.0.0",
    "@free-parking/shared": "workspace:*",
    "@react-native-async-storage/async-storage": "1.21.0",
    "@react-navigation/bottom-tabs": "^6.5.0",
    "@react-navigation/native": "^6.1.0",
    "@react-navigation/native-stack": "^7.9.0",
    "axios": "^1.6.0",
    "expo": "~54.0.0",
    "expo-asset": "^9.0.2",
    "expo-dev-client": "^6.0.20",
    "expo-font": "~11.10.3",
    "expo-location": "~16.5.5",
    "expo-router": "~3.4.10",
    "expo-splash-screen": "~0.26.4",
    "expo-status-bar": "~1.11.1",
    "fbjs": "^3.0.5",
    "memoize-one": "^6.0.0",
    "nullthrows": "^1.1.1",
    "react": "18.2.0",
    "react-native": "0.76.6",
    "react-native-gesture-handler": "~2.14.0",
    "react-native-reanimated": "~3.6.3",
    "react-native-safe-area-context": "4.8.2",
    "react-native-screens": "~3.29.0",
    "react-native-url-polyfill": "^2.0.0",
    "scheduler": "^0.27.0",
    "zustand": "^5.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "@babel/preset-react": "^7.23.0",
    "@babel/preset-typescript": "^7.23.0",
    "@types/react": "^18.2.79",
    "@types/react-native": "^0.73.0",
    "babel-jest": "^29.7.0",
    "eslint": "^8.54.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-native": "^4.1.0",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "metro": "^0.80.0",
    "metro-config": "^0.80.0",
    "typescript": "^5.2.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
