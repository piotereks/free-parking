{
  "name": "mobile",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start --offline",
    "android": "expo start --android --offline",
    "ios": "expo start --ios --offline",
    "web": "expo start --web --offline",
    "test": "jest",
    "test:watch": "jest --watch",
    "prebuild": "expo prebuild",
    "android:build": "cd android && gradlew assembleDebug",
    "android:install": "adb install android/app/build/outputs/apk/debug/app-debug.apk",
    "android:deploy": "cd android && gradlew installDebug"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.0",
    "@react-native-async-storage/async-storage": "^2.1.0",
    "expo": "~54.0.30",
    "expo-status-bar": "~3.0.9",
    "papaparse": "^5.5.3",
    "parking-shared": "file:../shared",
    "react": "19.1.0",
    "react-native": "0.81.5",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@testing-library/react-native": "^12.9.0",
    "babel-jest": "^29.7.0",
    "babel-preset-expo": "^54.0.9",
    "jest": "^29.7.0",
    "react-test-renderer": "19.1.0"
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(react-native|@react-native|expo|@expo|parking-shared)/)"
    ],
    "moduleNameMapper": {
      "^parking-shared$": "<rootDir>/../shared/src/index.js"
    },
    "testMatch": [
      "**/test/**/*.test.js"
    ]
  },
  "private": true
}
