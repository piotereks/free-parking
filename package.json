{
  "name": "parking",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "type": "module",
  "scripts": {
    "lint:web": "pnpm --filter ./packages/web run lint",
    "build:web": "pnpm --filter ./packages/web run build",
    "test:web": "pnpm --filter ./packages/web run test:run",
    "dev:web": "pnpm --filter ./packages/web run dev",
    "dev:mobile": "cd packages/mobile && npx expo start --offline",
    "dev:mobile:ios": "cd packages/mobile && npx expo start --ios --offline",
    "dev:mobile:android": "cd packages/mobile && npx expo start --android --offline",
    "build:mobile:ios": "pnpm --filter ./packages/mobile run build:ios",
    "build:mobile:android": "pnpm --filter ./packages/mobile run build:android",
    "build:mobile:preview": "pnpm --filter ./packages/mobile run build:preview",
    "submit:mobile:ios": "pnpm --filter ./packages/mobile run submit:ios",
    "submit:mobile:android": "pnpm --filter ./packages/mobile run submit:android",
    "test:mobile": "pnpm --filter ./packages/mobile run test",
    "android": "expo run:android",
    "ios": "expo run:ios"
  },
  "dependencies": {
    "eas-cli": "^16.28.0",
    "expo": "~54.0.30",
    "expo-dev-client": "~6.0.20",
    "invariant": "^2.2.4",
    "react": "19.1.0",
    "react-native": "0.81.5"
  }
}
