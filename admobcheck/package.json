    "android:deploy": "cd android && gradlew installRelease"
  },
  "dependencies": {
    "expo": "^54.0.31",
    "expo-status-bar": "^3.0.9",
    "expo-system-ui": "^6.0.9",
    "react": "^19.1.0",
    "react-native": "^0.81.5",
    "react-native-google-mobile-ads": "^16.0.1"
  },
  "private": true
}
