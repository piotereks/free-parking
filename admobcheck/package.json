{
  "name": "admobcheck",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest",
    "test:vitest": "vitest",
    "test:watch": "jest --watch",
    "lint": "eslint \"**/*.{js,jsx}\"",
